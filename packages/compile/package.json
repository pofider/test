{
  "name": "@jsreport/compile",
  "version": "3.0.3",
  "description": "Compile jsreport into single executable",
  "keywords": [
    "jsreport"
  ],
  "homepage": "https://github.com/jsreport/jsreport",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsreport/jsreport.git"
  },
  "author": "Jan Blaha",
  "main": "index.js",
  "bin": {
    "jsreport-compile": "./bin/compile.js"
  },
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "scripts": {
    "start": "node ./bin/compile.js",
    "test": "mocha test/test.js --timeout 480000 && standard"
  },
  "dependencies": {
    "@bjrmatos/pkg": "5.4.1",
    "acorn": "5.5.0",
    "bubleify": "0.7.0",
    "debug": "4.3.2",
    "lockfile": "1.0.4",
    "mkdirp": "0.5.5",
    "object-assign": "4.1.1",
    "rimraf": "2.6.3",
    "shortid": "2.2.15",
    "winser-with-api": "1.0.2",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "@jsreport/jsreport-cli": "3.2.2",
    "@jsreport/jsreport-core": "3.8.1",
    "fs-extra": "2.1.2",
    "mocha": "3.2.0",
    "should": "11.2.1",
    "standard": "16.0.4"
  },
  "standard": {
    "ignore": [
      "build",
      "test"
    ],
    "env": {
      "node": true,
      "mocha": true
    }
  }
}
