{
  "name": "@jsreport/jsreport-public-templates",
  "version": "3.0.0",
  "description": "jsreport extension for granting public access to particular templates",
  "keywords": [
    "jsreport"
  ],
  "homepage": "https://github.com/jsreport/jsreport/tree/master/packages/jsreport-public-templates",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsreport/jsreport.git"
  },
  "license": "LGPL",
  "author": {
    "name": "Jan Blaha"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "jsreport-studio-build",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha test --timeout=5000 && standard"
  },
  "dependencies": {
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@jsreport/jsreport-authentication": "3.3.2",
    "@jsreport/jsreport-authorization": "3.2.2",
    "@jsreport/jsreport-core": "3.8.1",
    "@jsreport/jsreport-express": "3.4.2",
    "@jsreport/studio-dev": "3.2.0",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "standard": "16.0.4",
    "supertest": "3.4.2"
  },
  "engines": {
    "node": ">=16.11"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}
